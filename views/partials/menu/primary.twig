{% if menu.primary.get_items %}
<nav class="nav-main desktop-nav" role="navigation">
	<ul class="nav">
	{% for item in menu.primary.get_items %}
		<li class="nav-item {{item.classes | join(' ')}}">
			<a class="nav-link" href="{{item.get_link}}">{{item.title}}</a>
		</li>
	{% endfor %}
	</ul>
</nav><!-- #nav -->
{% endif %}