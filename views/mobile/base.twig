	{% include 'partials/header-html.twig' %}

	<body class="{{body_class}}" data-template="base.twig">
		{{bn.analytics.google}}
		{{bn.analytics.raddwebstudio}}
		<div class="infinitishine-page-wrapper">
			<div class="page-inner">
				<header id="main-header">
					<div class="top-header">
						<div class="container d-none d-md-inline-block nav-wrapper">
							<nav id="nav-top" class="nav-top user-nav" role="navigation">
								<ul class="nav">
									{% for item in menu.user.get_items %}
										<li class="nav-item {{item.classes | join(' ')}}">
											
											<a class="nav-link" href="{{item.get_link}}">
												{% if 'home' in item.classes %}
													<span><i class="fa fa-home"></i></span>
												{% endif %}
												
												{% if 'account' in item.classes %}
													<span><i class="fa fa-user"></i></span>
												{% endif %}
												
												{% if 'phone' in item.classes %}
													<span><i class="fa fa-phone"></i></span>
												{% endif %}
												
												{% if 'cart' in item.classes %}
													<span><i class="fa fa-shopping-cart"></i></span>
													<span class="badge cart-count">
													{% if woocommerce.cart.get_cart_contents_count > 0 %}
														{{woocommerce.cart.get_cart_contents_count}}
													{% endif %}
													</span>
												{% endif %}
												
												{% if 'checkout' in item.classes %}
													<span><i class="fa fa-check"></i></span>
												{% endif %}
												
												{% if  'hide-title-text' not in item.classes %}
													{{item.title}} 
												{% endif %}
												
											</a>
											{% if item.get_children %}
												<ul class="nav nav-drop">
												{% for child in item.get_children %}
													<li class="nav-drop-item">
														<a class="nav-link" href="{{child.get_link}}">{{child.title}}</a>
													</li>
												{% endfor %}
												</ul>
											{% endif %}
										</li>
									{% endfor %}
									</ul>
							</nav><!-- #nav -->
						</div>
						
						<div class="mobile-menu-trigger-wrapper d-none d-lg-inline-block">
							<ul class="nav mobile-menu-trigger">
								<li class="nav-item">
									<a class="btn">
										<i class="fa fa-shopping-cart"></i>
										{% if woocommerce.cart.get_cart_contents_count > 0 %}
											{{woocommerce.cart.get_cart_contents_count}}
										{% else %}
											<span class="cart-count">0</span>
										{% endif %}
									</a>
								</li>
								<li class="nav-item">
									<a class="btn btn-menu">menu<i class="fa fa-bars"></i></a>
								</li>
							</ul>
						</div>
					</div>
					
					<div class="middle-headder">
						<div class=" container logo-title-wrapper">
							{%if options.logo %}
								<a class="hdr-logo-link" href="{{site.url}}" rel="home">
									<span class="logo-wrapper">
										<img class="logo img-fluid" src="{{options.logo|resize(600,400)}}"  />
									</span>
									
									{%if options.always_show_header %}
										<span class="site-title">{{site.title}}</span>
									{%endif%}
								</a>
								
							{%else%}
								<a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
							{%endif%}
						</div>
					</div>
					
					<div class="bottom-header d-none d-md-block">
						<div class="container">
							<div class="clearfix desktop-header-wrapper">
								{% include "partials/menu/primary.twig" %}
								{% include "partials/search/searchform.twig" %}
							</div>
						</div>
					</div>
				</header>
				
				<section class="{{container ? 'container' : ''}} content">
					<div class="canvas-overlay"></div>
					<div id="content" role="main" class="content-wrapper infinitishine-content-wrapper">
						
						<div class="{{show_sidebar ? 'col-sm-12 col-md-12 col-lg-9' : ''}} wrapper {{sidebar_class}}">
							{% block content %}
								<main class="container" role="main">
									Sorry, no content	
								</main>
							{% endblock %}
						</div>
						
						{% if show_sidebar %}
							{% include "partials/sidebar.twig" %}
						{%endif%}
	
					</div>
				</section><!-- END .infinitishine-canvas -->
				
				<footer class="infinitishine-footer-section">
					{% include 'partials/footer.twig' %}
				</footer>
			</div><!-- END  .page-inner -->
			
		{% include "partials/menu/mobile-primary.twig" %}
		
		</div>
		
		
		{{ function('wp_footer') }}
		<span class='hidden breakpoints'></span>
	</body>
</html>